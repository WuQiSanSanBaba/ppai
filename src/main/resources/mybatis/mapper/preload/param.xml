<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuqisan.ppai.preLoad.mapper.ParamMapper">
    <select id="getParamList" resultType="com.wuqisan.ppai.preLoad.bean.ParamInfo">
        select
        param_id,
        param_value
        from
        param_info
        where
        status=1
    </select>
</mapper>